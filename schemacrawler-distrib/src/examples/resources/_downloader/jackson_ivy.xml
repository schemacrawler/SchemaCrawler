<?xml version="1.0" encoding="UTF-8"?>
<ivy-module version="2.0"
            xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
            xsi:noNamespaceSchemaLocation="https://ant.apache.org/ivy/schemas/ivy.xsd">
  <info organisation="us.fatehi.schemacrawler" module="jackson"/>
  <configurations>
    <conf name="binaries" visibility="public" />
  </configurations>
  <dependencies>
    <dependency org="com.fasterxml.jackson.core"
                name="jackson-core" rev="2.15.3" conf="binaries->default" />
    <dependency org="com.fasterxml.jackson.datatype"
                name="jackson-datatype-jsr310" rev="2.15.3" conf="binaries->default" />
    <dependency org="com.fasterxml.jackson.core"
                name="jackson-databind" rev="2.15.3" conf="binaries->default" />
    <dependency org="com.fasterxml.jackson.dataformat"
                name="jackson-dataformat-yaml" rev="2.15.3" conf="binaries->default" />
    <dependency org="com.fasterxml.jackson.core"
                name="jackson-annotations" rev="2.15.3" conf="binaries->default" />
    <dependency org="org.yaml" name="snakeyaml" rev="2.1" conf="binaries->default" />

  </dependencies>
</ivy-module>
