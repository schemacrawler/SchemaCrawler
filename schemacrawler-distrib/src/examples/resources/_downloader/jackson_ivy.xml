<ivy-module version="2.0"
            xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
            xsi:noNamespaceSchemaLocation="https://ant.apache.org/ivy/schemas/ivy.xsd">
  <info organisation="us.fatehi.schemacrawler" module="jackson"/>
  <configurations>
    <conf name="binaries" visibility="public" />
  </configurations>
  <dependencies>
    <dependency org="tools.jackson.core"
                name="jackson-core" rev="3.0.2" conf="binaries->default" />
    <dependency org="tools.jackson.core"
                name="jackson-databind" rev="3.0.2" conf="binaries->default" />
    <dependency org="tools.jackson.core"
                name="jackson-dataformat-yaml" rev="3.0.2" conf="binaries->default" />

    <dependency org="com.fasterxml.jackson.core"
                name="jackson-annotations" rev="2.20" conf="binaries->default" />
    <dependency org="org.yaml" name="snakeyaml" rev="2.5" conf="binaries->default" />

  </dependencies>
</ivy-module>
