<?xml version="1.0" encoding="UTF-8"?>
<ivy-module version="2.0"
            xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
            xsi:noNamespaceSchemaLocation="https://ant.apache.org/ivy/schemas/ivy.xsd">
  <info organisation="us.fatehi.schemacrawler" module="groovy"/>
  <configurations>
    <conf name="binaries" visibility="public" />
  </configurations>
  <dependencies>
    <dependency org="org.codehaus.groovy" name="groovy-all"
                rev="3.0.18" conf="binaries->default" />

    <exclude org="org.codehaus.groovy" module="groovy-ant" />
    <exclude org="org.codehaus.groovy" module="groovy-cli-picocli" />
    <exclude org="org.codehaus.groovy" module="groovy-test" />
    <exclude org="org.codehaus.groovy" module="groovy-test-junit5" />
    <exclude org="org.codehaus.groovy" module="groovy-testng" />
    <exclude org="jline" module="jline" />
    <exclude org="com.beust" module="jcommander" />
  </dependencies>
</ivy-module>
