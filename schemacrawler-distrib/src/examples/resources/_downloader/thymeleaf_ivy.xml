<ivy-module version="2.0"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="https://ant.apache.org/ivy/schemas/ivy.xsd">
  <info organisation="us.fatehi.schemacrawler" module="thymeleaf"/>
  <configurations>
    <conf name="binaries" visibility="public" />
  </configurations>
  <dependencies>
    <dependency org="org.thymeleaf" name="thymeleaf"
      rev="3.1.3.RELEASE" conf="binaries->default" />

    <exclude org="javax.servlet" module="servlet-api" />
    <exclude org="org.slf4j" />
    <exclude org="com.fasterxml.jackson.core" />
    <exclude org="com.fasterxml.jackson.datatype" />
  </dependencies>
</ivy-module>
