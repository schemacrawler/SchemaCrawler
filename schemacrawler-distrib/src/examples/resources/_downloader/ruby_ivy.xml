<ivy-module version="2.0"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="https://ant.apache.org/ivy/schemas/ivy.xsd">
  <info organisation="us.fatehi.schemacrawler" module="ruby"/>
  <configurations>
    <conf name="binaries" visibility="public" />
  </configurations>
  <dependencies>
    <dependency org="org.jruby" name="jruby"
              rev="9.4.13.0" conf="binaries->default" />

    <override org="com.github.jnr" module="jnr-ffi"
              rev="2.2.17" />
    <override org="com.github.jnr" module="jnr-posix"
              rev="3.1.20" />
    <override org="org.ow2.asm"
              rev="9.8" />

  </dependencies>
</ivy-module>
